{
  "name": "books",
  "version": "1.0.0",
  "description": "Book collection manager",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/Trungi/books",
  "license": "MIT",
  "scripts": {
    "api:build": "tsc -p tsconfig.api.json",
    "api:start": "node dist/api.js",
    "api:start:dev": "ts-node-dev src/api.ts",
    "build": "npm run api:build",
    "dev": "run-p mongod:start api:start:dev",
    "fix:lint": "eslint src --ext .ts --fix",
    "mongod:start": "ts-node -e \"require('./src/scripts/start-mongod.ts').startMongod()\"",
    "postinstall": "",
    "swagger": "swagger project start src/api.yaml",
    "start": "npm run api:start",
    "test": "jest"
  },
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.3.3",
    "get-port": "^5.1.1",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.1",
    "mongodb": "^3.6.2",
    "mongodb-memory-server": "^6.9.2",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "^26.4.2",
    "ts-node-dev": "^1.0.0",
    "typescript-rest": "^3.0.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/get-port": "^4.2.0",
    "@types/mongodb": "^3.5.31",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  }
}
